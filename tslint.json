{
  "defaultSeverity": "error",
  "extends": "tslint-config-airbnb",
  "jsRules": {
    "jsdoc-format": true,
    "valid-jsdoc": true
  },
  "rules": {
    "semicolon": false,
    "ter-arrow-parens": false,
    "align": false,
    "space-before-function-paren": false,
    "import-name": false
  },
  "rulesDirectory": [],
  "tslint.packageManager": "yarn"
}
